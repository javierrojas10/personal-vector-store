{
  "name": "personal-vector-store",
  "version": "0.1.0",
  "description": "An open standard for portable personal identity representations in LLMs",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "generate": "ts-node src/generate_pvs.ts",
    "test": "jest",
    "benchmark": "ts-node benchmarks/run_benchmark.ts",
    "benchmark:help": "ts-node benchmarks/run_benchmark.ts --help",
    "benchmark:list": "ts-node benchmarks/run_benchmark.ts --list-models",
    "setup": "node setup.js",
    "example": "ts-node examples/basic_usage.ts",
    "analyze-tokens": "ts-node src/cli_token_analyzer.ts",
    "token-demo": "ts-node examples/token_analysis_demo.ts"
  },
  "keywords": ["ai", "llm", "vector", "embeddings", "personal-context"],
  "author": "Javier Rojas",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "dependencies": {
    "openai": "^4.20.1",
    "dotenv": "^16.3.1",
    "ora": "^5.4.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3"
  }
}
